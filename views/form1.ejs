    <div>
      <input type="password" class="form-control p-3" placeholder="Password" name="password">
      <p class="err"><%=p%></p>
    </div>

    <button name="button" value="form1" class="mt-4 w-100 btn btn-lg btn-primary" type="submit">Sign in</button>
  </form>
